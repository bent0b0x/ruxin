{
  "name": "ruxin",
  "version": "0.0.0",
  "description": "Redux/React code manager",
  "main": "index.js",
  "repository": "https://github.com/bent0b0x/ruxin.git",
  "author": "Benjamin Baum",
  "license": "MIT",
  "scripts": {
    "lint": "eslint scripts",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "precommit": "lint-staged",
    "prep": "mkdir build/",
    "clean": "rm -rf build/",
    "dev": "yarn run clean && yarn run prep && webpack --watch --hot --progress --colors",
    "build": "yarn run clean && yarn run prep && NODE_ENV=production webpack --progress --colors",
    "test": "mocha-webpack --webpack-config webpack.config.js \"test/**/*.js\"",
    "test:dev": "yarn test -- --watch"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "babel-generator": "^6.26.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babylon": "^6.18.0",
    "camelcase": "^4.1.0",
    "lodash.findlastindex": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.map": "^4.6.0",
    "mocha-webpack": "^0.7.0",
    "redux": "^3.7.2",
    "to-ast": "^1.0.0",
    "webpack": "^3.5.6"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-object-rest-spread": "^0.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.7.1"
  }
}
